<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
	
		<bean class="converters.AdminToStringConverter" /> 
  		<bean class="converters.StringToAdminConverter" /> 
	
		<bean class="converters.ClassificationToStringConverter" /> 
  		<bean class="converters.StringToClassificationConverter" /> 
  		
  		<bean class="converters.CoachToStringConverter" /> 
  		<bean class="converters.StringToCoachConverter" /> 
  		
  		<bean class="converters.CommentToStringConverter" /> 
  		<bean class="converters.StringToCommentConverter" /> 
  		
  		<bean class="converters.FamilyToStringConverter" /> 
  		<bean class="converters.StringToFamilyConverter" /> 
  		
  		<bean class="converters.FolderToStringConverter" /> 
  		<bean class="converters.StringToFolderConverter" /> 
  		
  		<bean class="converters.MatchToStringConverter" /> 
  		<bean class="converters.StringToMatchConverter" /> 
  		
  		<bean class="converters.MessageToStringConverter" /> 
  		<bean class="converters.StringToMessageConverter" /> 
  		
  		<bean class="converters.PlayerToStringConverter" /> 
  		<bean class="converters.StringToPlayerConverter" /> 
  		
  		<bean class="converters.RecruitmentToStringConverter" /> 
  		<bean class="converters.StringToRecruitmentConverter" /> 
  		
  		<bean class="converters.SquadraToStringConverter" /> 
  		<bean class="converters.StringToSquadraConverter" /> 
  		
  		<bean class="converters.UserToStringConverter" /> 
  		<bean class="converters.StringToUserConverter" /> 
  				  			
	</util:list>

</beans>